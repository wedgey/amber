<p id="notice"><%= notice %></p>

<h1>Farms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Farm code</th>
      <th>Lat</th>
      <th>Lng</th>
      <th>Layout</th>
      <th>Size</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @farms.each do |farm| %>
      <tr>
        <td><%= farm.name %></td>
        <td><%= farm.farm_code %></td>
        <td><%= farm.lat %></td>
        <td><%= farm.lng %></td>
        <td><%= farm.layout %></td>
        <td><%= farm.size %></td>
        <td><%= link_to 'Show', farm %></td>
        <td><%= link_to 'Edit', edit_farm_path(farm) %></td>
        <td><%= link_to 'Destroy', farm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Farm', new_farm_path %>


